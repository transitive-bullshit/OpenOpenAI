{
  "name": "openopenai",
  "private": true,
  "description": "Self-hosted version of OpenAI's new stateful Assistants API.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "repository": "transitive-bullshit/OpenOpenAI",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@8.10.2",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "prebuild": "run-s clean && prisma generate",
    "clean": "del dist",
    "dev": "tsc --watch",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "release": "np",
    "test": "run-p test:*",
    "test:format": "prettier --check \"**/*.{js,ts,tsx}\"",
    "test:lint": "eslint .",
    "test:unit": "vitest",
    "test:typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.2.0",
    "@hono/zod-openapi": "^0.8.3",
    "@prisma/client": "^5.5.2",
    "hono": "^3.9.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@dexaai/eslint-config": "^0.4.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@types/node": "^20.9.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.53.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "np": "^8.0.4",
    "npm-run-all": "^4.1.5",
    "openai": "^4.17.3",
    "openapi-jsonschema-parameters": "^12.1.3",
    "openapi-types": "^12.1.3",
    "prettier": "^2.8.8",
    "prisma": "^5.5.2",
    "prisma-json-types-generator": "^3.0.3",
    "quicktype-core": "^23.0.77",
    "tsx": "^4.0.0",
    "type-fest": "^4.7.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --ignore-unknown --write"
    ]
  },
  "keywords": [
    "openai",
    "ai",
    "assistant",
    "gpt",
    "agent",
    "agi",
    "llms",
    "chatgpt"
  ]
}
